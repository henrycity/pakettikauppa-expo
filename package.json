{
  "name": "pakettikauppa-expo",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "yarn jest --watchAll",
    "format": "yarn prettier --write .",
    "lint": "yarn eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/src/jest/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native-picker/.*|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "coverageReporters": [
      "text-summary",
      "text",
      "cobertura"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-picker/picker": "^1.9.4",
    "@react-navigation/bottom-tabs": "^5.8.0",
    "@react-navigation/drawer": "^5.11.3",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "dotenv": "^8.2.0",
    "expo": "~39.0.2",
    "expo-application": "~2.3.0",
    "expo-asset": "~8.2.0",
    "expo-auth-session": "~2.0.0",
    "expo-constants": "~9.2.0",
    "expo-device": "^2.3.0",
    "expo-font": "~8.3.0",
    "expo-linking": "^1.0.1",
    "expo-localization": "~9.0.0",
    "expo-splash-screen": "~0.6.2",
    "expo-status-bar": "~1.0.2",
    "expo-web-browser": "~8.5.0",
    "firebase": "^7.8.2",
    "googleapis": "^66.0.0",
    "i18next": "^19.8.4",
    "jest-enzyme": "^7.1.2",
    "lodash.debounce": "^4.0.8",
    "query-string": "^7.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^6.15.1",
    "react-i18next": "^11.7.4",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.4.tar.gz",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-paper": "^4.7.2",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "3.1.4",
    "react-native-screens": "~2.10.1",
    "react-native-web": "^0.14.9",
    "react-native-web-hover": "^0.2.9",
    "swr": "^0.3.9",
    "use-debounce": "^6.0.0",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/react-native": "^7.1.0",
    "@types/jest": "^26.0.19",
    "@types/lodash.debounce": "^4.0.6",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/react-test-renderer": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "cypress": "^6.2.1",
    "cypress-react-selector": "^2.2.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-universe": "^6.0.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-expo": "~39.0.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "typedoc": "^0.19.2",
    "typescript": "~3.9.5"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc --noEmit"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write",
      "yarn lint",
      "jest --bail --findRelatedTests"
    ]
  }
}
